<main class="p-5">
    <div class="card mb-5">
        <div class="card-body">
            <div class="row">
                <div class="col-6">
                    <h1 class="text-start">Безсмертний Андрій</h1>
                </div>
                <div class="col-6">
                    <h2 class="text-end">Resume project</h2>
                </div>
            </div>
        </div>
    </div>
    <div class="py-5">
        <h2>Список сторінок</h2>
    </div>
    <div class="list-group">
        <div class="row">
            <div class="col-3">
                {{#with navbuttons}}
                <div class="list-group-item">
                    {{#each project}}
                    <div class="my-2">
                        <a href="{{href}}" class="btn btn-primary">{{name}}</a>
                    </div>
                    {{/each}}
                </div>
                {{/with}}
            </div>
        </div>
    </div>
</main>