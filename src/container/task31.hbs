<main>
    <section class="col-12">
        <div class="row">
            {{#with navigation}}
            <div class="col-6">
                {{#each links}}
                <a href="{{url}}" class="btn btn-link">{{text}}</a>
                {{/each}}
            </div>
            <div class="col-6">
                {{#with button}}
                <div>
                    <a href="{{url}}" class="btn btn-primary">{{text}}</a>
                </div>
                {{/with}}
            </div>
            {{/with}}
        </div>
    </section>
    <hr>
    <section class="row">
        <div class="col-3">
            {{#with header}}
            <h1>{{title}}</h1>
            <p>{{description}}</p>
            {{#with button}}
            <div>
                <a href="{{link}}" class="btn btn-primary">{{text}}</a>
            </div>
            {{/with}}
            {{/with}}
        </div>
    </section>
    <hr>
    <div class="col-12">
        <div class="row g-2">
            {{#with city}}
            <h2>{{title}}</h2>
            {{#each cards}}
            <div class="card col-5 text-bg-success">
                <img class="card-img-top" src="https://picsum.photos/400/200" alt="{{alt}}">
                <h6 class="card-title">{{title}}</h6>
                <p>{{description}}</p>
                {{#with button}}
                <div>
                    <a href="{{href}}" class="btn btn-primary">{{text}}</a>
                </div>
                {{/with}}
            </div>
            {{/each}}
            {{/with}}
        </div>
    </div>
    <hr>
    <section>
        {{#with winner}}
        <h2>{{title}}</h2>
        {{#with table}}
        <table class="table table-striped table-bordered">
            {{#with head}}
            <thead>
                <tr>
                    <th>
                        {{this}}
                    </th>
                </tr>
            </thead>
            {{/with}}
            {{#each user}}
            <tbody>
                <tr>
                    <th>{{this}}</th>

                </tr>
            </tbody>
            {{/each}}
        </table>
        {{/with}}
        {{/with}}
    </section>
</main>