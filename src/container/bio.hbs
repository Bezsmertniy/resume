gi

<head>
    <section>
        <h1>{{name}}</h1>
        <div>{{birthdate}} - {{deathdate}}</div>
        <br>
        <div>birthplace: {{birthplace}}</div>
        <div>deathplace: {{deathplace}}</div>
        <br>
        <div>nationality: {{nationality}}</div>
        <br>
        <div>occupation: {{occupation}}</div>
        <br>
        <div>known_for:
            <ul>
                {{#each known_for}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
        </div>
    </section>
</head>
<hr>

<body>
    <h2>Education:</h2>
    <div>
        {{#each education}}
        <div>{{degree}}</div>
        <div>{{field}}</div>
        <div>{{institution}}</div>
        <div>{{year}}</div>
        {{/each}}
    </div>
    <br>
    <h3>Notable_publications</h3>
    <div>
        {{#each notable_publications}}
        <div>{{title}}</div>
        <div>{{year}}</div>
        <div>{{publisher}}</div>
        <br>
        {{/each}}
    </div>
    <br>
    <h3>Partners</h3>
    <div>
        {{#each partners}}
        <div>{{name}}</div>
        <div>{{relationship}}</div>
        <div>{{years}}</div>
        <br>
        {{/each}}
    </div>
    <br>
    <h3>Awards</h3>
    <div>
        {{#each awards}}
        <div>{{title}}</div>
        <div></div>{{year}}
    </div>
    <div></div>{{description}}</div>
    {{/each}}
    </div>
    <br>
    <h3>Influences</h3>
    <ul>
        {{#each influences}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <br>
    <h3>Influenced</h3>
    <ul>
        {{#each influenced}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <br>
    <h3>Quotes</h3>
    <ul>
        {{#each quotes}}
        <li><q>{{this}}</q></li>
        {{/each}}
    </ul>
    <br>
    <h3>Major discoveries</h3>
    <div>
        {{#each major_discoveries}}
        <div>{{title}}</div>
        <div>{{year}}</div>
        <p>({{description}})</p>
        <br>
        {{/each}}
    </div>
    <br>
    <h3>Contributions</h3>
    <div>{{contributions.title}}</div>
    <p>({{contributions.description}})</p>
    <div>{{contributions.year}}</div>
    <div>{{contributions.field}}</div>

</body>