<main class="m-5">
    <section class="text-center my-5">
        {{#with page_header}}
        <h1>{{title}}</h1>
        <p>{{description}}</p>
        {{/with}}
    </section>
    <section>
        {{#with section}}
        <div class="card mb-5">
            <div class="card-body">
                {{#with about_us}}
                <div class="text-end m-3">
                    {{#with title_list}}
                    <div class="display-6">{{main}}</div>
                    <h2 class="pt-3">{{sub}}</h2>
                    {{/with}}
                </div>
                <div class="p-3">
                    {{#with button}}
                    <div class="btn btn-outline-info">{{text}}</div>
                    {{/with}}
                </div>
                {{/with}}
            </div>
        </div>
        <div class="card mt-5">
            {{#with advantage}}
            <div class="card-body">
                <div class="display-5">{{title}}</div>
                <div class="card my-4">
                    {{#with efficiency}}
                    <div class="card-body">
                        <div class="display-4 mb-3">{{title}}</div>
                        <h6 class="my-3">{{sub_title}}</h6>
                        <p class="mb-3">{{description}}</p>
                        <div>
                            <a href="#" class="btn btn-outline-warning">{{button}}</a>
                        </div>
                    </div>
                    {{/with}}
                </div>
                <div class="card my-4">
                    {{#with innovation}}
                    <div class="card-body">
                        <div class="text-center">
                            <div class="display-4">{{title}}</div>
                            <h6 class="my-3">{{sub_title}}</h6>
                            <p class="mb-3">{{description}}</p>
                        </div>
                        <div>
                            <a href="#" class="btn btn-outline-warning">{{button}}</a>
                        </div>
                        {{/with}}
                    </div>
                </div>
                <div class="card mb-2">
                    {{#with collaboration}}
                    <div class="card-body">
                        <div class="text-end">
                            <div class="display-4">{{title}}</div>
                            <h6 class="my-3">{{sub_title}}</h6>
                            <p class="mb-3">{{description}}</p>
                        </div>
                        <div class="mt-4">
                            <a href="#" class="btn btn-outline-warning">{{button}}</a>
                        </div>
                    </div>
                    {{/with}}
                </div>
            </div>
        </div>
        {{/with}}
        </div>
        <div class="card mt-5">
            {{#with agreement}}
            <div class="card-body">
                <div class="display-5">{{title}}</div>
                <div class="card mt-3">
                    {{#with info}}
                    <div class="card-body">
                        <h5>{{sub_title}}</h5>
                        <div class="card mt-3">
                            {{#with detail}}
                            <div class="card-body">
                                <p>{{text}}</p>
                                <div>
                                    <a href="#" class="btn btn-success">{{button}}</a>
                                </div>
                            </div>
                            {{/with}}
                        </div>
                    </div>
                    {{/with}}
                </div>
                {{/with}}
            </div>
        </div>
        {{/with}}

    </section>
</main>