<main class="p-5">
    <h1 class="text-center">Web</h1>
    <h2 class="my-3 ms-4">Languages</h2>
    <div class="my-3">
        <hr>
    </div>
    <div>
        {{#with web}}
        <div class="ms-2">
            {{#each languages}}
            <h3 class="mt-3">{{name}}</h3>
            <div class="my-2">{{version}}</div>
            <div>({{description}})</div>
            <ul class="mt-3">
                {{#each elements}}
                <li>
                    <h6>{{name}}</h6>
                    <div class="my-3">({{description}})</div>
                    <div class="ms-2">attributes:
                        {{#each attributes}}
                        <div class="my-2">{{name}}</div>
                        <div class="ms-3">({{description}})</div>
                        {{/each}}
                    </div>
                </li>
                {{/each}}
            </ul>
            <div class="mt-4 mb-3">
                {{#each properties}}
                <b>{{name}}</b>
                <div class="my-2 ms-2">({{description}})</div>
                <div class="my-3">values:
                    {{#each values}}
                    <div class="ms-2 my-2">{{value}}</div>
                    <div class="ms-3">({{description}})</div>
                    {{/each}}
                </div>
                {{/each}}
            </div>
            <div class="ms-2">
                {{#each functions}}
                <h5 class="my-4">{{name}}</h5>
                <div class="my-2 ms-2">({{description}})</div>
                <div class="ms-3">
                    <h6>parameters:</h6>
                    {{#each parameters}}
                    <div class="ms-1">{{parameters}}</div>
                    <div class="ms-2 my-2">{{type}}</div>
                    <div class="ms-3">({{description}})</div>
                    {{/each}}
                </div>
                {{/each}}
            </div>
            {{/each}}
        </div>
        {{/with}}
    </div>
</main>